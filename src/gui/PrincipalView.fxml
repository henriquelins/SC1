                  <TableColumn fx:id="tableColumnDetalhes" editable="false" prefWidth="400.0" style="-fx-alignment: CENTER;" text="Detalhes" />
              </columns>
            </TableView>
            <ToolBar minHeight="-Infinity" prefHeight="30.0" prefWidth="1034.0" VBox.vgrow="NEVER">
