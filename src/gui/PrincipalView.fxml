                        <Insets />
                     </opaqueInsets>
                  </TextField>
                  <Button fx:id="buttonPesquisar" defaultButton="true" mnemonicParsing="false" onAction="#onBtPesquisarClienteAction" prefHeight="30.0" prefWidth="90.0" style="-fx-background-radius: 10;" text="PESQUISA" />
