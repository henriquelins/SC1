<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PrincipalFormController">
   <content>
      <VBox alignment="CENTER">
         <children>
            <MenuBar fx:id="menuBarPrincipal" minHeight="-Infinity">
               <menus>
                  <Menu mnemonicParsing="false" text="Arquivo">
                     <items>
                        <MenuItem fx:id="menuItemLogout" mnemonicParsing="false" onAction="#onMenuItemLogoutAction" text="Logout" />
                        <MenuItem fx:id="menuItemSair" mnemonicParsing="false" onAction="#onMenuItemSairAction" text="Sair" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Cadastro">
                     <items>
                        <MenuItem fx:id="menuItemUsuario" mnemonicParsing="false" onAction="#onMenuItemUsuarioAction" text="Usuário" />
                        <MenuItem fx:id="menuItemCliente" mnemonicParsing="false" onAction="#onMenuItemClienteAction" text="Cliente" />
                        <MenuItem fx:id="menuItemVendedor" mnemonicParsing="false" onAction="#onMenuItemVendedorAction" text="Vendedor" />
                        <MenuItem fx:id="menuItemUnidade" mnemonicParsing="false" onAction="#onMenuItemUnidadeAction" text="Unidade" />
                        <MenuItem fx:id="menuItemProduto" mnemonicParsing="false" onAction="#onMenuItemProdutoAction" text="Produto" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Ajuda">
                     <items>
                        <MenuItem fx:id="menuItemAjuda" mnemonicParsing="false" onAction="#onMenuItemAjudaAction" text="Sobre" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <ToolBar minHeight="-Infinity" prefHeight="50.0" prefWidth="1091.0">
              <items>
                  <Button fx:id="buttonNovoCliente" mnemonicParsing="false" onAction="#onBtClienteAction" prefHeight="25.0" prefWidth="107.0" text="Novo Cliente" />
                  <TextField fx:id="textFieldPesquisar" accessibleRole="IMAGE_VIEW" prefHeight="27.0" prefWidth="307.0" promptText="Pesquisar Clientes">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </TextField>
                  <Button fx:id="buttonPesquisar" defaultButton="true" mnemonicParsing="false" onAction="#onBtPesquisarClienteAction" prefHeight="25.0" prefWidth="71.0" text="Pesquisar" />
                  <ComboBox fx:id="comboBoxListarClientes" onAction="#onComboBoxListarClientesAction" prefHeight="27.0" prefWidth="247.0" promptText="Clientes (Todos / Limpar / Vendedores )" />
                  <Button fx:id="buttonExportar" mnemonicParsing="false" onAction="#onBtExportarAction" prefHeight="26.0" prefWidth="150.0" text="Exportar para Excel" />
                  <Button fx:id="buttonImprimir" mnemonicParsing="false" onAction="#onBtImprimirAction" prefHeight="26.0" prefWidth="150.0" text="Imprimir Listagem" />
              </items>
            </ToolBar>
            <AnchorPane minHeight="-Infinity" prefHeight="102.0" prefWidth="1091.0" style="-fx-background-color: gray;">
               <children>
                  <TextArea fx:id="textAreaDetalhes" editable="false" layoutX="14.0" layoutY="11.0" prefHeight="78.0" prefWidth="646.0" style="-fx-background-color: white;" />
                  <TextArea fx:id="textAreaSaldoCnpj" editable="false" layoutX="682.0" layoutY="11.0" prefHeight="78.0" prefWidth="399.0" style="-fx-background-color: white;" />
               </children>
            </AnchorPane>
            <TableView fx:id="tableViewCliente" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#onMouseClickedAction" prefHeight="492.0" prefWidth="1019.0" VBox.vgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="tableColumnIndex" editable="false" prefWidth="30.0" style="-fx-alignment: CENTER;" text="#" />
                <TableColumn fx:id="tableColumnId" editable="false" prefWidth="60.0" style="-fx-alignment: CENTER;" text="Código" />
                  <TableColumn fx:id="tableColumnNomeFantasia" editable="false" prefWidth="300.0" style="-fx-alignment: CENTER;" text="Nome Fantasia" />
                  <TableColumn fx:id="tableColumnRazaoSocial" editable="false" prefWidth="300.0" style="-fx-alignment: CENTER;" text="Razão Social" />
                  <TableColumn fx:id="tableColumnCnpj" editable="false" prefWidth="150.0" style="-fx-alignment: CENTER;" text="CNPJ" />
                  <TableColumn fx:id="tableColumnAtendimento" editable="false" prefWidth="150.0" style="-fx-alignment: CENTER;" text="Unidade" />
                  <TableColumn fx:id="tableColumnEmail" editable="false" prefWidth="270.0" style="-fx-alignment: CENTER;" text="Email" />
                  <TableColumn fx:id="tableColumnFone" editable="false" minWidth="0.0" prefWidth="100.0" style="-fx-alignment: CENTER;" text="Fone Celular" />
              </columns>
            </TableView>
            <ToolBar minHeight="-Infinity" prefHeight="30.0" prefWidth="1034.0" VBox.vgrow="NEVER">
               <items>
                  <AnchorPane prefHeight="0.0" prefWidth="1006.0">
                     <children>
                        <Label fx:id="labelLogado" layoutY="-1.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="303.0">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </Label>
                     </children>
                  </AnchorPane>
               </items>
            </ToolBar>
         </children>
      </VBox>
   </content>
</ScrollPane>
